      "automation",
      "workflow",
      "no-code",
      "integration"
    ],
    "requirements": {
      "memory": "512MB",
      "cpu": "0.5"
    },
    "healthcheck": {
      "endpoint": "/api/v1/flags",
      "interval": 30,
      "timeout": 10
    }
